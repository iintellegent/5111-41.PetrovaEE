argument1 = int(input("Введите число:"))
argument2 = int(input("Введите число:"))

print(argument1,argument2, sep = " ")

print(argument1 + argument2)
print(argument1 - argument2)
print(argument1 * argument2)
print(argument1 / argument2)
import math as mt
print(argument1 % argument2)
print(mt.log10(argument1))
print(argument1 ** argument2)


